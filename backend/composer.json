{
    "name": "nellai-iptv/backend",
    "description": "Nellai IPTV REST API Backend",
    "version": "1.31.0",
    "type": "project",
    "require": {
        "php": "^8.0",
        "firebase/php-jwt": "^5.5",
        "illuminate/database": "^10.0",
        "illuminate/events": "^10.0",
        "illuminate/pagination": "^10.0",
        "monolog/monolog": "^2.9",
        "php-di/php-di": "^6.4",
        "php-di/slim-bridge": "^3.2",
        "ramsey/uuid": "^4.2",
        "razorpay/razorpay": "^2.9",
        "resend/resend-php": "^1.1",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.10",
        "vlucas/phpdotenv": "^5.5",
        "vlucas/valitron": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "fakerphp/faker": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public"
    }
}
